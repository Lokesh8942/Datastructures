#include <stdio.h>

int main() {
    int num, sum = 0;

    printf("Enter numbers \n");

    do {
        
        scanf("%d", &num);

        
        if (num % 2 != 0 && num != 0) {
            sum += num;
        }
    } while (num != 0);  

    
    printf("Sum of odd numbers: %d\n", sum);

    return 0;
}
